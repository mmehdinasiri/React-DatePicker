{
  "name": "react-calendar-datetime-picker",
  "version": "1.6.3",
  "description": "Made with create-react-library",
  "author": "MehdiNasiri",
  "license": "MIT",
  "repository": "https://github.com/mmehdinasiri/react-calendar-datetime-picker",
  "homepage": "https://mmehdinasiri.github.io/react-calendar-datetime-picker/",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "keywords": [
    "persian date picker",
    "date picker",
    "date and time picker",
    "calendar date picker",
    "persian date time picker"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "example": "cd ./example && npm run start",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run example\" ",
    "delete-dist": "rimraf ./dist",
    "delete-cjs-file": "rimraf ./dist/index.js",
    "microbundle-crl": "npm run delete-dist && microbundle-crl --compress --format es,modern,cjs --css-modules false --sourcemap false ",
    "build": "npm run delete-dist && rollup -c ",
    "start": "npm run delete-dist && microbundle-crl watch --no-compress --format modern,cjs --css-modules false ",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^2.0.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jalaali-js": "^1.1.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-sass": "4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^5.5.0",
    "sass": "^1.30.0",
    "sass-rem": "^2.0.1",
    "typescript": "^4.3.5"
  },
