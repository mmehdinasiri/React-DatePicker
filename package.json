{
  "name": "react-calendar-datetime-picker",
  "version": "1.6.3",
  "description": "A fast and small calendar for react ",
  "author": "MehdiNasiri",
  "license": "MIT",
  "repository": "https://github.com/mmehdinasiri/react-calendar-datetime-picker",
  "homepage": "https://mmehdinasiri.github.io/react-calendar-datetime-picker/",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "keywords": [
    "persian date picker",
    "date picker",
    "date and time picker",
    "calendar date picker",
    "persian date time picker"
  ],
  "scripts": {
    "init:husky": "npx husky-init && husky install && husky set .husky/pre-commit 'npm run typecheck'",
    "dev:example": "cd ./vite && npm run dev",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run dev:example\" ",
    "delete-dist": "rimraf ./dist",
    "delete-cjs-file": "rimraf ./dist/index.js",
    "start": "vite",
    "build:watch": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:latest": "npm run build && npm run preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:styles": "stylelint --allow-empty-input \"**/*.scss\"",
    "lint:fix": "npm run lint:fix-ts && npm run lint:fix-styles",
    "lint:fix-ts": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:fix-styles": "stylelint --allow-empty-input \"**/*.scss\" --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "typecheck": "tsc --noEmit",
    "push": "git push --follow-tags",
    "release": "standard-version",
    "release:first": "npm run release -- --first-release && npm run push",
    "release:major": "npm run release -- --release-as major && npm run push",
    "release:minor": "npm run release -- --release-as minor && npm run push",
    "release:notag": "npm run release -- --no-git-tag-version && npm run push",
    "release:patch": "npm run release -- --release-as patch && npm run push"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "dependencies": {
    "jalaali-js": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@cypress/react": "^5.12.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jalaali-js": "^1.1.0",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clsx": "^1.1.1",
    "concurrently": "^7.2.2",
    "cypress": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.52.3",
    "standard-version": "^9.5.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-dts": "^1.2.1",
    "vite-plugin-remove-console": "^1.0.4",
    "vite-plugin-svgr": "^2.2.0",
    "vitest": "^0.14.2"
  }
}
